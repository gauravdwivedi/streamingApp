n        </div>)\n    }\n\n    onSubmit=(formvalues)=>{\n        //console.log(formvalues)\n\n        this.props.createStream(formvalues);\n\n    }\n\n    render(){\n        // console.log(this.props)\n        return (\n            <form onSubmit={this.props.handleSubmit(this.onSubmit)} className=\"ui form error \">\n                <Field name=\"title\" component={this.renderInput} label =\"Enter Title\"/>\n                <Field name=\"description\" component={this.renderInput} label={\"Enter Description\"}/>\n                <button className=\"button ui primary\">Submit</button>\n\n            </form>\n        ) \n\n    }\n}\n\nconst validate =(formValues)=>{\n    const errors ={};\n    if(!formValues.title){\n        //only ran if the user did not enter the title\n        errors.title='You must enter a title';\n    }\n\n    if(!formValues.description){\n        errors.description='You must enter a description'\n    }\n\n\n    return errors;\n\n}\n\n\nconst formWrapped = reduxForm({\n    form:'streamCreate',\n    validate\n})(StreamCreate);\n\n\nexport default connect(null,{createStream})(formWrapped);","/home/gaurav/Projects/udemy/prod/streams/client/src/components/streams/StreamShow.js",[],"/home/gaurav/Projects/udemy/prod/streams/client/src/components/streams/StreamEdit.js",[],"/home/gaurav/Projects/udemy/prod/streams/client/src/components/streams/StreamList.js",[],"/home/gaurav/Projects/udemy/prod/streams/client/src/components/streams/StreamDelete.js",[],"/home/gaurav/Projects/udemy/prod/streams/client/src/components/Header.js",[],"/home/gaurav/Projects/udemy/prod/streams/client/src/components/GoogleAuth.js",[],"/home/gaurav/Projects/udemy/prod/streams/client/src/reducers/index.js",[],"/home/gaurav/Projects/udemy/prod/streams/client/src/actions/index.js",[],"/home/gaurav/Projects/udemy/prod/streams/client/src/reducers/authReducer.js",[],"/home/gaurav/Projects/udemy/prod/streams/client/src/actions/types.js",[],"/home/gaurav/Projects/udemy/prod/streams/client/src/apis/streams.js",[],"/home/gaurav/Projects/udemy/prod/streams/client/src/reducers/streamReducer.js",["72"],"import _ from 'lodash';\n\nimport {\n    FETCH_STREAM,\n    FETCH_STREAMS,\n    CREATE_STREAM,\n    EDIT_STREAM,\n    DELETE_STREAM\n} from '../actions/types';\n\n\nexport default (state={},action)=>{\n    switch(action.type){\n\n        //fetching all records\n        case FETCH_STREAMS:\n            return {...state, ..._.mapKeys(action.payload,'id')}\n        //for single record \n        case FETCH_STREAM:\n            return {...state,[action.payload.id]:action.payload}\n        \n        case CREATE_STREAM:\n            return {...state,[action.payload.id]:action.payload}\n\n        case EDIT_STREAM:\n            return {...state,[action.payload.id]:action.payload}\n\n        //deleting\n        case DELETE_STREAM:\n            return _.omit(state,action.payload);\n        \n        default:\n            return state;\n    }\n}","/home/gaurav/Projects/udemy/prod/streams/client/src/history.js",[],{"ruleId":"73","replacedBy":"74"},{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","severity":1,"message":"78","line":2,"column":16,"nodeType":"79","messageId":"80","endLine":2,"endColumn":26},{"ruleId":"81","severity":1,"message":"82","line":12,"column":1,"nodeType":"83","endLine":35,"endColumn":2},"no-native-reassign",["84"],"no-negated-in-lhs",["85"],"no-unused-vars","'formValues' is defined but never used.","Identifier","unusedVar","import/no-anonymous-default-export","Assign arrow function to a variable before exporting as module default","ExportDefaultDeclaration","no-global-assign","no-unsafe-negation"]